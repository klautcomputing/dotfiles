! Cursor
URxvt*cursorColor: #86a2b0
URxvt*colorUL: #86a2b0

!term propertys
*saveLines: 10000
urxvt*font: xft:terminus
!URxvt.font: xft:monospace:pixelsize=14:antialias=true

!urxvt only propertys
URxvt*scrollBar: false
URxvt*secondaryScroll: true
URxvt*transparent: True
URxvt*fadeColor: black
URxvt*tintColor: black

URxvt.tabbed.tabbar-fg: 3
URxvt.tabbed.tabbar-bg: blue
URxvt.tabbed.tab-fg: 0
URxvt.tabbed.tab-bg: 1

URxvt*visualBell: false
URxvt*urgentOnBell: true

!fix color issues
URxvt.intensityStyles: false

!path for extensions
URxvt.perl-lib: /home/leex/dotfiles/generated/urxvt

!plugins
URxvt.perl-ext-common: default,matcher,url-select,resize-font

!url
URxvt.urlLauncher:   /usr/bin/google-chrome
URxvt.keysym.M-u:    perl:url-select:select_next
URxvt.underlineURLs: true

!font size
URxvt.keysym.M-plus:        perl:resize-font:bigger
URxvt.keysym.M-minus:       perl:resize-font:smaller
URxvt.keysym.M-numbersign:  perl:resize-font:reset
URxvt.keysym.M-question:    perl:resize-font:show

!scroll
! do not scroll with output
URxvt.scrollTtyOutput: false
! scroll in relation to buffer (with mouse scroll or Shift+Page Up)
URxvt.scrollWithBuffer: true
! scroll back to the bottom on keypress
URxvt.scrollTtyKeypress: true

URxvt*matcher.button:   1
URxvt.matcher.pattern.1:  ^(/[^:]+):(\\d*)
URxvt.matcher.launcher.1: urxvt -e vim +$2 $1
